      <h1>CALCULATOR</h1>


 <div class="container">
        <div id="screen">0</div>
        <div class="keys" onclick="(-">+/-</div>
        <div class="keys">()</div>
        <div class="keys" onclick="%">%</div>
        <div class="keys operator" onclick="/">/</div>
        <div class="keys numpad" onclick="7">7</div>
        <div class="keys numpad" onclick="8">8</div>
        <div class="keys numpad" onclick="9">9</div>
        <div class="keys operator" onclick="*">x</div>
        <div class="keys numpad" onclick="4">4</div>
        <div class="keys numpad" onclick="5">5</div>
        <div class="keys numpad" onclick="6">6</div>
        <div class="keys operator" onclick="+">+</div>
        <div class="keys numpad" onclick="1">1</div>
        <div class="keys numpad" onclick="2">2</div>
        <div class="keys numpad" onclick="3">3</div>
        <div class="keys operator" onclick="-">-</div>
        <div class="keys numpad" onclick="0">0</div>
        <div class="keys numpad" onclick=".">.</div>
        <div class="keys" id="clear">C</div>
        <div class="keys" id="equals">=</div>
    </div>
